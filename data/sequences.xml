<odoo>
    <data>
        <record id="sequence_pelanggan" model="ir.sequence">
            <field name="name">Pelanggan Sequence</field>
            <field name="code">pelanggan</field>
            <field name="prefix">PLG/%(year)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>